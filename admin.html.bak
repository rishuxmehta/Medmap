                                    <button class="action-btn view"><i class="fas fa-eye"></i></button>
                                    <button class="action-btn delete"><i class="fas fa-trash"></i></button>
                                </td>
                            </tr>
                            <tr>
                                <td>Bhailal Amin General Hospital</td>
                                <td>Gorwa Road, Vadodara, Gujarat</td>
                                <td>
                                    <div class="capacity-indicator">
                                        <div class="capacity-bar-container">
                                            <div class="capacity-bar" style="width: 62%"></div>
                                        </div>
                                        <span>62% (31/50)</span>
                                    </div>
                                </td>
                                <td>8 mins</td>
                                <td><span class="status-badge available">Available</span></td>
                                <td>+91 265 2280300</td>
                                <td class="actions-cell">
                                    <button class="action-btn edit"><i class="fas fa-edit"></i></button>
                                    <button class="action-btn view"><i class="fas fa-eye"></i></button>
                                    <button class="action-btn delete"><i class="fas fa-trash"></i></button>
                                </td>
                            </tr>
                            <tr>
                                <td>Baroda Medical College Hospital</td>
                                <td>Raopura, Vadodara, Gujarat</td>
                                <td>
                                    <div class="capacity-indicator">
                                        <div class="capacity-bar-container">
                                            <div class="capacity-bar" style="width: 78%"></div>
                                        </div>
                                        <span>78% (23/30)</span>
                                    </div>
