<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin Panel - Smart Ambulance Routing System</title>
    <link rel="stylesheet" href="css/styles.css">
    <link rel="stylesheet" href="css/admin.css">
    <link rel="stylesheet" href="css/notifications.css">
    <link rel="stylesheet" href="https://unpkg.com/leaflet@1.7.1/dist/leaflet.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700&display=swap" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
</head>
<body>
    <div class="app-container admin-container">
        <!-- Header Section -->
        <header>
            <div class="logo">
                <i class="fas fa-ambulance"></i>
                <h1>Med<span>Map</span></h1>
            </div>
            <nav>
                <ul>
                    <li><a href="index.html">Home</a></li>
                    <li><a href="dashboard.html">Dashboard</a></li>
                    <li><a href="admin.html" class="active">Admin</a></li>
                </ul>
            </nav>
            <div class="mobile-menu-toggle">
                <i class="fas fa-bars"></i>
            </div>
        </header>

        <!-- Admin Sidebar -->
        <aside class="admin-sidebar">
            <div class="admin-profile">
                <div class="admin-avatar">
                    <i class="fas fa-user-shield"></i>
                </div>
                <div class="admin-info">
                    <h3>Admin User</h3>
                    <p>System Administrator</p>
                </div>
            </div>
            <nav class="admin-nav">
                <ul>
                    <li class="active" data-tab="dashboard">
                        <i class="fas fa-tachometer-alt"></i>
                        <span>Dashboard</span>
                    </li>
                    <li data-tab="ambulances">
                        <i class="fas fa-ambulance"></i>
                        <span>Ambulances</span>
                    </li>
                    <li data-tab="hospitals">
                        <i class="fas fa-hospital"></i>
                        <span>Hospitals</span>
                    </li>
                    <li data-tab="emergencies">
                        <i class="fas fa-exclamation-circle"></i>
                        <span>Emergencies</span>
                    </li>
                    <li data-tab="routes">
                        <i class="fas fa-route"></i>
                        <span>Routes</span>
                    </li>
                    <li data-tab="users">
                        <i class="fas fa-users"></i>
                        <span>Users</span>
                    </li>
                    <li data-tab="updates">
                        <i class="fas fa-bell"></i>
                        <span>Recent Updates</span>
                        <span class="update-badge">3</span>
                    </li>
                    <li data-tab="settings">
                        <i class="fas fa-cog"></i>
                        <span>Settings</span>
                    </li>
                </ul>
            </nav>
            <div class="admin-logout">
                <button>
                    <i class="fas fa-sign-out-alt"></i>
                    <span>Logout</span>
                </button>
            </div>
        </aside>

        <!-- Main Content -->
        <main class="admin-main">
            <!-- Dashboard Tab -->
            <section class="admin-tab active" id="dashboard-tab">
                <div class="admin-header">
                    <h2>Admin Dashboard</h2>
                    <div class="admin-actions">
                        <button class="refresh-btn"><i class="fas fa-sync-alt"></i> Refresh</button>
                        <div class="date-range">
                            <input type="date" id="start-date">
                            <span>to</span>
                            <input type="date" id="end-date">
                            <button class="apply-btn">Apply</button>
                        </div>
                    </div>
                </div>

                <div class="stats-cards">
                    <div class="stat-card">
                        <div class="stat-icon">
                            <i class="fas fa-ambulance"></i>
                        </div>
                        <div class="stat-info">
                            <h3>Total Ambulances</h3>
                            <p class="stat-value">24</p>
                            <p class="stat-change positive">+2 from last month</p>
                        </div>
                    </div>
                    <div class="stat-card">
                        <div class="stat-icon">
                            <i class="fas fa-hospital"></i>
                        </div>
                        <div class="stat-info">
                            <h3>Hospitals</h3>
                            <p class="stat-value">12</p>
                            <p class="stat-change">No change</p>
                        </div>
                    </div>
                    <div class="stat-card">
                        <div class="stat-icon">
                            <i class="fas fa-exclamation-circle"></i>
                        </div>
                        <div class="stat-info">
                            <h3>Emergencies Today</h3>
                            <p class="stat-value">18</p>
                            <p class="stat-change negative">+5 from yesterday</p>
                        </div>
                    </div>
                    <div class="stat-card">
                        <div class="stat-icon">
                            <i class="fas fa-clock"></i>
                        </div>
                        <div class="stat-info">
                            <h3>Avg. Response Time</h3>
                            <p class="stat-value">8.2 min</p>
                            <p class="stat-change positive">-0.5 min from last week</p>
                        </div>
                    </div>
                </div>

                <div class="admin-row">
                    <div class="admin-col">
                        <div class="admin-card">
                            <div class="card-header">
                                <h3>Active Emergencies</h3>
                                <button class="view-all-btn">View All</button>
                            </div>
                            <div class="emergency-list">
                                <div class="emergency-item high">
                                    <div class="emergency-info">
                                        <h4>Cardiac Arrest</h4>
                                        <p>123 Main St, Downtown</p>
                                        <p class="emergency-time">Started: 10:23 AM</p>
                                    </div>
                                    <div class="emergency-status">
                                        <span class="status-badge">In Progress</span>
                                        <p>ETA: 3 min</p>
                                    </div>
                                </div>
                                <div class="emergency-item medium">
                                    <div class="emergency-info">
                                        <h4>Traffic Accident</h4>
                                        <p>456 Oak Ave, Westside</p>
                                        <p class="emergency-time">Started: 10:15 AM</p>
                                    </div>
                                    <div class="emergency-status">
                                        <span class="status-badge">Dispatched</span>
                                        <p>ETA: 7 min</p>
                                    </div>
                                </div>
                                <div class="emergency-item low">
                                    <div class="emergency-info">
                                        <h4>Respiratory Distress</h4>
                                        <p>789 Pine St, Eastside</p>
                                        <p class="emergency-time">Started: 10:05 AM</p>
                                    </div>
                                    <div class="emergency-status">
                                        <span class="status-badge">At Hospital</span>
                                        <p>Arrived: 10:22 AM</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="admin-col">
                        <div class="admin-card">
                            <div class="card-header">
                                <h3>System Overview</h3>
                                <div class="card-actions">
                                    <button class="card-action"><i class="fas fa-download"></i></button>
                                    <button class="card-action"><i class="fas fa-expand"></i></button>
                                </div>
                            </div>
                            <div class="admin-map" id="admin-overview-map"></div>
                        </div>
                    </div>
                </div>

                <div class="admin-row">
                    <div class="admin-col">
                        <div class="admin-card">
                            <div class="card-header">
                                <h3>Hospital Capacity</h3>
                                <div class="card-actions">
                                    <button class="card-action"><i class="fas fa-download"></i></button>
                                    <button class="card-action"><i class="fas fa-expand"></i></button>
                                </div>
                            </div>
                            <div class="hospital-capacity-chart">
                                <canvas id="hospital-capacity-chart"></canvas>
                            </div>
                        </div>
                    </div>
                    <div class="admin-col">
                        <div class="admin-card">
                            <div class="card-header">
                                <h3>Response Time Trends</h3>
                                <div class="card-actions">
                                    <button class="card-action"><i class="fas fa-download"></i></button>
                                    <button class="card-action"><i class="fas fa-expand"></i></button>
                                </div>
                            </div>
                            <div class="response-time-chart">
                                <canvas id="response-time-chart"></canvas>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

            <!-- Ambulances Tab -->
            <section class="admin-tab" id="ambulances-tab">
                <div class="admin-header">
                    <h2>Ambulance Management</h2>
                    <div class="admin-actions">
                        <button class="add-btn" id="add-ambulance-btn"><i class="fas fa-plus"></i> Add Ambulance</button>
                        <button class="view-btn" id="view-ambulance-locations"><i class="fas fa-map-marked-alt"></i> View Locations</button>
                        <button class="refresh-btn"><i class="fas fa-sync-alt"></i> Refresh</button>
                        <div class="search-box">
                            <input type="text" placeholder="Search ambulances...">
                            <i class="fas fa-search"></i>
                        </div>
                    </div>
                </div>

                <div class="data-table-container">
                    <table class="data-table ambulance-table">
                        <thead>
                            <tr>
                                <th>ID</th>
                                <th>Status</th>
                                <th>Type</th>
                                <th>Location</th>
                                <th>Driver</th>
                                <th>Equipment</th>
                                <th>Last Updated</th>
                                <th>Actions</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>
                                    <div class="ambulance-id">
                                        <i class="fas fa-ambulance"></i>
                                        <span>AMB-1042</span>
                                    </div>
                                </td>
                                <td><span class="status-badge available">Available</span></td>
                                <td>Type II</td>
                                <td>Alkapuri, Vadodara</td>
                                <td>
                                    <div class="driver-info">
                                        <div class="user-avatar">RS</div>
                                        <div>Rajesh Singh</div>
                                    </div>
                                </td>
                                <td>
                                    <div class="equipment-info">
                                        <span class="equipment-badge">AED</span>
                                        <span class="equipment-badge">Oxygen</span>
                                        <span class="equipment-badge">+3</span>
                                    </div>
                                </td>
                                <td>2 mins ago</td>
                                <td class="actions-cell">
                                    <button class="action-btn edit"><i class="fas fa-edit"></i></button>
                                    <button class="action-btn view"><i class="fas fa-eye"></i></button>
                                    <button class="action-btn delete"><i class="fas fa-trash"></i></button>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div class="ambulance-id">
                                        <i class="fas fa-ambulance"></i>
                                        <span>AMB-2157</span>
                                    </div>
                                </td>
                                <td><span class="status-badge dispatched">Dispatched</span></td>
                                <td>Type III</td>
                                <td>En route to Fatehgunj, Vadodara</td>
                                <td>
                                    <div class="driver-info">
                                        <div class="user-avatar">SP</div>
                                        <div>Sunil Patel</div>
                                    </div>
                                </td>
                                <td>
                                    <div class="equipment-info">
